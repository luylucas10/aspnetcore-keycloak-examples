{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
	  "realm": "aspnet",
	  "auth-server-url": "http://localhost:8080/",
	  "ssl-required": "none",
	  "resource": "aspnet-api",
	  "public-client": true,
	  "verify-token-audience": true,
	  "use-resource-role-mappings": true,
	  "confidential-port": 0
  }
}
